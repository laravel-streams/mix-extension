{
    "name": "@laravel-streams/mix-extension",
    "version": "1.0.0",
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "types": "dist/index.d.ts",
    "main": "dist/index.js",
    "scripts": {
        "clean": "npm-run-all clean:dist clean:js clean:doc",
        "clean:dist": "rimraf ./dist",
        "clean:js": "rimraf ./{src}/**/*.js",
        "build": "npm-run-all clean compile",
        "compile": "tsc -p tsconfig.build.json && mv ./out/src/ dist && rm -rf ./out"
    },
    "peerDependencies": {
        "@babel/core": "^7.15.8",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/preset-env": "^7.15.8",
        "laravel-mix": "*",
        "ts-lib": "^0.0.5",
        "ts-loader": "^9.2.6",
        "ts-node": "^10.4.0",
        "typescript": "^4.4.4",
        "webpack": "*"
    }
}
